"http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>

  <!-- Servlets -->
  <servlet>
    <servlet-name>XMLProxyServlet</servlet-name>
    <servlet-class>de.saumya.gwt.datamapper.server.ProxyServlet</servlet-class>
